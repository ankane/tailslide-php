{
    "name": "ankane/tailslide",
    "description": "Median and percentile for Eloquent / Laravel",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ankane/tailslide-php/issues",
        "source": "https://github.com/ankane/tailslide-php"
    },
    "authors": [
        {
            "name": "Andrew Kane",
            "email": "andrew@ankane.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tailslide\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">= 8.0",
        "illuminate/database": ">= 9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tailslide\\TailslideServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "illuminate/database": ">= 9.0",
        "illuminate/events": ">= 9.0"
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
